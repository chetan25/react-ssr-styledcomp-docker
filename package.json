{
  "name": "ssr-react-docker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "serve": "cross-env NODE_ENV=development webpack serve --config ./webpack.client.config.js",
    "clean": "rimraf build && rimraf public",
    "build": "npm-run-all --parallel clean build:* && node build/serverBundel.js",
    "dev:server": "nodemon --watch build --exec \"node build/serverBundel.js\"",
    "dev:build-server": "cross-env NODE_ENV=development webpack --config webpack.server.config.js --watch",
    "dev:build-client": "cross-env NODE_ENV=development webpack --config webpack.client.config.js --watch",
    "start:old": "nodemon --watch ./src --exec \"npm run build && node ./build/serverBundel.js \"",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.js",
    "test": "nodemon test.js",
    "dev": "npm-run-all --parallel dev:*"
  },
  "nodemonConfig": {
    "legacyWatch": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-container-query": "^0.13.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.1.2",
    "react-router": "^6.16.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^6.15.0",
    "redux": "^4.2.1",
    "reselect": "^4.1.8",
    "serialize-javascript": "^6.0.1",
    "styled-components": "^6.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "babel-cli": "^6.26.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^3.0.1",
    "nodemon-webpack-plugin": "^4.8.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "style-loader": "^3.3.3",
    "transform-class-properties": "^1.0.0-beta",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0",
    "webpack-node-externals": "^3.0.0"
  }
}
